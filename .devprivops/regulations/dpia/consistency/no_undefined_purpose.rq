PREFIX dpia: <https://devprivops.com/dpia/>

SELECT ?purpose
WHERE {
    {
        dpia:ROOT dpia:personal_data/dpia:purposes ?purpose .
    } UNION {
        dpia:ROOT dpia:personal_data_processing_whitelist ?purpose .
    } UNION {
        dpia:ROOT dpia:personal_data_processing_that_requires_DPIA ?purpose .
    } 

    FILTER NOT EXISTS {
        dpia:ROOT dpia:processings ?processing .
        ?processing dpia:purposes ?purpose .
    }
}