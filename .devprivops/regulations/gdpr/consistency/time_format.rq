PREFIX dfd: <https://devprivops.com/dfd/>

SELECT ?s ?o
WHERE {
    ?s dfd:validity|dfd:periodicity ?o .
    FILTER NOT EXISTS {
        FILTER(
            REGEX(?o, "^[0-9]+(s|sec|secs|second|seconds|m|min|mins|minute|minutes|h|hr|hrs|hour|hours|d|day|days|w|week|weeks|month|months|y|year|years)$") || 
            ?o = "eternal"
        )
    }
}