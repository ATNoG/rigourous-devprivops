PREFIX dfd: <https://devprivops.com/dfd/>

SELECT ?df ?dest ?data
WHERE {
    dfd:ROOT dfd:data_flows ?df .
    ?df dfd:to ?dest .
    ?df dfd:data ?data

    FILTER NOT EXISTS {
        {
            ?dest dfd:consumes ?data .
        } UNION {
            ?dest dfd:data_stored/dfd:type ?data .
        }
    }
}